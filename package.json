{
	"name": "timeline-of-artists",
	"version": "1.0.0",
	"description": "Timeline of artists",
	"main": "index.js",
	"scripts": {
		"start": "nodemon server.js",
		"build-js": "browserify src/scripts/bundle.js -o public/scripts/bundle.js -t [ babelify --presets [ env ] --plugins [ transform-object-rest-spread ] ]",
		"build-css": "node-sass -w --output-style src/styles/*/*.scss public/styles/main.css",
		"dev": "parallelshell \"npm run start\" \"npm run build-js\" \"npm run build-css\"",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"author": "Mees Rutten",
	"license": "MIT",
	"dependencies": {
		"body-parser": "^1.17.1",
		"ejs": "^2.5.7",
		"express": "^4.15.2",
		"http": "0.0.0",
		"multer": "^1.3.0",
		"nodemon": "^1.17.2",
		"parallelshell": "^3.0.2",
		"request": "^2.81.0"
	},
	"devDependencies": {
		"babel-plugin-transform-object-rest-spread": "^6.26.0",
		"browserify": "^14.1.0",
		"babelify": "^8.0.0",
		"babel-preset-env": "^1.6.1",
		"node-sass": "^4.7.2"
	}
}
